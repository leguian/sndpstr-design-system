@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

i{{baseSelector}} {
    display: inline-block;
    line-height: normal;
    font-size: 1.5em;
    height: 1em;
    width: 1em;
    text-align: center;
    vertical-align: middle;
    speak: none;
    margin-top: -2px;
}

{{baseSelector}}:before {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
    text-rendering: auto;
    -webkit-font-smoothing: antialiased;
}

{{#each codepoints}}
i.{{../classPrefix}}.{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
