@font-face {
	font-family: "{{fontName}}";
	src: {{{src}}};
}

i{{baseSelector}} {
    line-height: normal;
    font-size: 140%;
    vertical-align: middle;
    height: 1em;
    width: 1em;
}

{{baseSelector}}:before {
	font-family: {{fontName}} !important;
	font-style: normal;
	font-weight: normal !important;
	vertical-align: middle;
}

{{#each codepoints}}
i.{{../classPrefix}}.{{@key}}:before {
	content: "\\{{this}}";
}
{{/each}}
